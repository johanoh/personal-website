<turbo-frame id="content">
    <h1>Projects</h1>

    <div class="projects">
        <% @projects.each do |project| %>
        <div class="project">
            <!-- Link to each project -->
            <%= link_to project.name, project_path(project.id), data: { turbo_frame: "content" } %>
        </div>
        <% end %>
    </div>
</turbo-frame>